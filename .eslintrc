{
	"extends": "airbnb-base",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"rules": {
		"comma-dangle": [
			2,
			"never"
		],
		"arrow-parens": [
			2,
			"as-needed"
		],
		"linebreak-style": 0,
		"padded-blocks": 0,
		"indent": [
			1,
			"tab"
		],
		"no-console": 0,
		"no-underscore-dangle": [
			2,
			{
				"allow": [
					"_id",
					"__v"
				]
			}
		],
		"no-param-reassign": [
			2,
			{
				"props": false
			}
		],
		"object-shorthand": [
			2
		],
		"new-cap": [
			2,
			{
				"capIsNewExceptions": [
					"Router",
					"ObjectId"
				]
			}
		],
		"no-tabs": 0,
		"max-len": [
			1,
			120
		],
		"no-plusplus": 0,
		"no-await-in-loop": 0
	},
	"env": {
		"node": true,
		"es6": true
	}
}